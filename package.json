{
	"name": "yapi-file-help",
	"displayName": "Yapi File Help",
	"description": "Yapi文件创建工具",
	"version": "0.0.6",
	"publisher": "SunRising",
	"icon": "asset/logo.png",
	"repository": "https://github.com/SunRisingChang/yapi-file-help.git",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"workspaceContains:**/yapi.conf.json"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts"
	},
	"contributes": {
		"commands": [
			{
				"command": "extension.addApiFile",
				"title": "Generate api.ts By Yapi"
			},
			{
				"command": "extension.addInterfaceFile",
				"title": "Generate interfaces.ts By Yapi"
			},
			{
				"command": "extension.refresh",
				"title": "刷新",
				"icon": "$(refresh)"
			},
			{
				"command": "extension.copyAllApi",
				"title": "生成全部 Api 至剪贴板",
				"icon": "$(clippy)"
			},
			{
				"command": "extension.findApi",
				"title": "查找 Api",
				"icon": "$(search)"
			},
			{
				"command": "extension.resetApiList",
				"title": "重置 Api 列表",
				"icon": "$(discard)"
			},
			{
				"command": "extension.showApiTitle",
				"title": "显示 Api 标题",
				"icon": "$(list-filter)"
			},
			{
				"command": "extension.showApiPath",
				"title": "显示 Api 路径",
				"icon": "$(list-flat)"
			},
			{
				"command": "extension.copyApi",
				"title": "生成 Api 至剪贴板",
				"icon": "$(files)"
			},
			{
				"command": "extension.copyInterFace",
				"title": "生成 Generic 至剪贴板",
				"icon": "$(json)"
			},
			{
				"command": "extension.expandAll",
				"title": "展开全部",
				"icon": "$(expand-all)"
			},
			{
				"command": "extension.collapseAll",
				"title": "收起全部",
				"icon": "$(collapse-all)"
			},
			{
				"command": "extension.openUrl",
				"title": "详情"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.addApiFile",
					"when": "button.addApiFile",
					"group": "y_commands@1"
				},
				{
					"command": "extension.addInterfaceFile",
					"when": "button.addInterfaceFile",
					"group": "y_commands@2"
				}
			],
			"view/title": [
				{
					"command": "extension.findApi",
					"when": "button.findApi",
					"group": "navigation@1"
				},
				{
					"command": "extension.resetApiList",
					"when": "button.resetApiList",
					"group": "navigation@2"
				},
				{
					"command": "extension.showApiTitle",
					"when": "button.showApiPath",
					"group": "navigation@3"
				},
				{
					"command": "extension.showApiPath",
					"when": "button.showApiTitle",
					"group": "navigation@4"
				},
				{
					"command": "extension.copyAllApi",
					"when": "button.copyAllApi",
					"group": "navigation@5"
				},
				{
					"command": "extension.refresh",
					"group": "navigation@6"
				},
				{
					"command": "extension.expandAll",
					"when": "button.collapseAll",
					"group": "navigation@7"
				},
				{
					"command": "extension.collapseAll",
					"when": "button.expandAll",
					"group": "navigation@8"
				}
			],
			"view/item/context": [
				{
					"command": "extension.copyApi",
					"when": "viewItem == file",
					"group": "inline"
				},
				{
					"command": "extension.copyInterFace",
					"when": "viewItem == file",
					"group": "inline"
				},
				{
					"command": "extension.openUrl",
					"when": "viewItem == file",
					"group": "navigation@3"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "yapiList",
					"title": "Yapi To Angular",
					"icon": "asset/Yapi.svg"
				}
			]
		},
		"views": {
			"yapiList": [
				{
					"id": "yapi-menu-view",
					"name": "接口清单"
				}
			]
		}
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/lodash": "^4.14.165",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/prettier": "^2.1.5",
		"@types/vscode": "^1.51.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	},
	"dependencies": {
		"@types/fs-extra": "^9.0.5",
		"axios": "^0.21.0",
		"clipboardy": "^2.3.0",
		"fs-extra": "^9.0.1",
		"handlebars": "^4.7.6",
		"json-schema-to-typescript": "^10.0.0"
	}
}