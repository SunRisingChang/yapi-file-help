{
	"name": "yapi-file-help",
	"displayName": "Yapi File Help",
	"description": "Yapi文件创建工具",
	"version": "0.0.1",
	"publisher": "SunRising",
	"icon": "asset/logo.png",
	"repository": "https://github.com/SunRisingChang/yapi-file-help.git",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts"
	},
	"contributes": {
		"commands": [
			{
				"command": "extension.addApiFile",
				"title": "Generate api.ts By Yapi"
			},
			{
				"command": "extension.addInterfaceFile",
				"title": "Generate interfaces.ts By Yapi"
			},
			{
				"command": "extension.refresh",
				"title": "刷新",
				"icon": "$(refresh)"
			},
			{
				"command": "extension.copyApi",
				"title": "生成 api 至剪贴板"
			},
			{
				"command": "extension.copyInterFace",
				"title": "生成 typing 至剪贴板"
			}
		],
		"menus": {
			"explorer/context": [
				{
					"command": "extension.addApiFile",
					"group": "y_commands@1"
				},
				{
					"command": "extension.addInterfaceFile",
					"group": "y_commands@2"
				}
			],
			"view/title": [
				{
					"command": "extension.refresh",
					"group": "navigation"
				}
			],
			"view/item/context": [
				{
					"command": "extension.copyApi",
					"when": "viewItem == file",
					"group": "navigation@1"
				},
				{
					"command": "extension.copyInterFace",
					"when": "viewItem == file",
					"group": "navigation@2"
				}
			]
		},
		"viewsContainers": {
			"activitybar": [
				{
					"id": "yapiList",
					"title": "Yapi File Help",
					"icon": "asset/Yapi.svg"
				}
			]
		},
		"views": {
			"yapiList": [
				{
					"id": "yapi-file-help-menu-view",
					"name": "接口清单"
				}
			]
		}
	},
	"devDependencies": {
		"@types/vscode": "^1.51.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.15.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	},
	"dependencies": {
		"@types/fs-extra": "^9.0.5",
		"axios": "^0.21.0",
		"clipboardy": "^2.3.0",
		"fs-extra": "^9.0.1",
		"handlebars": "^4.7.6",
		"json-schema-to-typescript": "^10.0.0"
	}
}